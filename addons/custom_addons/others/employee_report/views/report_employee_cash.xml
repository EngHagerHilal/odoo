<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
                <template id="employee_cash_reciept_report">
                    <t t-call="web.html_container">
                         <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"/>
                <span>  company_id </span>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"/>
               
            </t>
            
            <div class="header">
                <div class="row mt32 mb32">
                    <div class="col-6">
                        <img t-if="company.logo" t-att-src="'data:image/png;base64,%s' % to_text(company.logo)" style="max-height: 50px;"/>
                    </div>
                    <div class="col-6 text-right" style="font:12px lucida-console,sans-serif !important;">
                        <span t-field="company.partner_id"/>
                        <br/>
                        <span t-field="company.partner_id.street"/>
                        <br/>
                        <span t-field="company.partner_id.street2"/>
                        <br/>
                        <span t-field="company.partner_id.city"/>
                        <br/>
                        <span t-field="company.partner_id.country_id"/>
                        <br/>
                        <span t-field="company.partner_id.vat"/>
                        <br/>
                    </div>
                </div>
            </div>
                    <t t-foreach="docs" t-as="doc">
                        <div class="container" style="direction: rtl;font-size:14 !important;right-margin:75px !important;">
  <h1> HELLLLLLLLLLLLLLLO !!!!!!!!!!!!</h1>
                        </div>
                    </t>
                    </t>
                </template>
</data>
</odoo>
