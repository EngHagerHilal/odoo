<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
    <template id="detailed_taxes_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <div class="oe_structure"/>
                    <div class="text-left">
                    <h3 class="text-center"> Detailed Tax Invoice  </h3>
                    <!--<span t-esc="data['product_id']"/>-->
                    <t t-foreach="in" t-as="invoice">
                        <span t-field="invoice.amount_total"/>
                        <span t-field="invoice.amount_tax"/>
                        <span t-field="invoice.partner_id.name"/>

                    </t>
                    <div t-field="total_in" />
                    </br>
                    </br>
                    <t t-foreach="out" t-as="invoice">
                        <span t-field="invoice.amount_total"/>
                        <span t-field="invoice.amount_tax"/>
                        <span t-field="invoice.partner_id.name"/>

                    </t>
                    <div t-field="total_out" />
                    </div>

                </div>
            </t>
        </t>
    </template>
</data>
</odoo>